import sys
from src.frontends.tml_frontend import *

from redbaron import RedBaron
import re


def parse_tml(src):

    prog = None
    with open(src, "r") as source:
        prog = RedBaron(source)

    return prog


def main():
    source = sys.argv[1]
    fst = parse_tml(source)
    prog = process_FST(fst)

    print prog.debug_print()
